<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joesoft.ticketslogger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".technicians.NewTechnicianActivity"
            android:label="@string/new_technician" />
        <activity
            android:name=".technicians.UsersActivity"
            android:label="@string/technicians" />
        <activity
            android:name=".technicians.UserDetailsActivity"
            android:label="@string/edit_technician" />
        <activity
            android:name=".issues.NewIssueActivity"
            android:label="@string/new_issue" />
        <activity
            android:name=".issues.IssuesActivity"
            android:label="@string/issues" />
        <activity
            android:name=".issues.IssueDetailsActivity"
            android:label="@string/edit_issue" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>